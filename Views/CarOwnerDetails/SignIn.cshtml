@model OnlineCarParkingBookingManagement.Models.CarOwnerLogin_Model

@{
    ViewBag.Title = "SignIn";
}

<h2><i><mark>Sign In</mark></i></h2>
@Html.ValidationSummary(true, "")
@using (Html.BeginForm("SignIn", "CarOwnerDetails", FormMethod.Post))
{
    <table cellpadding="10">
        <tr>
            <td>
                @Html.Label("User Name")
            </td>
            <td>
                @Html.TextBoxFor(Model => Model.emailId, new { MaxLength = 30 })
                @Html.ValidationMessageFor(Model => Model.emailId)
            </td>
        </tr>
        <tr>
            <td>
                @Html.Label("Password")
            </td>
            <td>
                @Html.PasswordFor(Model => Model.password, new { MaxLength = 10 })
                @Html.ValidationMessageFor(Model => Model.password)
            </td>
        </tr>
        <tr>
            <td>
                <input type="submit" value="Login" />
            </td>
        </tr>
    </table>
    @Html.ActionLink("Edit Car Owner Details", "EditCarOwnerDetails", "CarOwnerDetails")
    <div>
        @Html.Partial("Home_PartialView")
    </div>
    <div>
        @{
            Html.RenderPartial("RenderPartialView");
        }
    </div>
}

